    <body>
        <!-- Loader -->
        <div class="load-inner">
            <div class="showbox">
                <div class="loader">
                    <svg class="circular" viewBox="25 25 50 50">
                        <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10"/>
                    </svg>
                    <span class="logo"><img src="<%= path %>img/png/loader.png" alt="logo"></span>
                </div>
            </div>
        </div>
        <!-- Loader end -->
        <!--start modal-->
        <div class="modal fade" id="modalOrder" tabindex="5" role="dialog"  aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-body">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="30" height="30" viewBox="0 0 20 20">
                                <path fill="#ffffff" d="M10.707 10.5l5.646-5.646c0.195-0.195 0.195-0.512 0-0.707s-0.512-0.195-0.707 0l-5.646 5.646-5.646-5.646c-0.195-0.195-0.512-0.195-0.707 0s-0.195 0.512 0 0.707l5.646 5.646-5.646 5.646c-0.195 0.195-0.195 0.512 0 0.707 0.098 0.098 0.226 0.146 0.354 0.146s0.256-0.049 0.354-0.146l5.646-5.646 5.646 5.646c0.098 0.098 0.226 0.146 0.354 0.146s0.256-0.049 0.354-0.146c0.195-0.195 0.195-0.512 0-0.707l-5.646-5.646z"></path>
                            </svg>
                        </button>
                        <div class="textCenter">
                            <h4 class="title">Обратная связь</h4>
                        </div>
                        <div class="block-form">
                            <form action="/callback">
                                <div class="group">
                                    <input type="text" name="name" placeholder="Ваше имя">
                                </div>
                                <div class="group">
                                    <input type="tel" name="name" placeholder="Телефон">
                                </div>
                                <div class="textCenter">
                                    <div class="goup-btn">
                                        <button type="submit" class="btn_main">Отправить</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--end modal-->

        <!--Wrapper-->
        <div class="wrapper">
            <!--start Header-->
            <header id="header" class="header">
               <div class="top_head">
                   <div class="container">
                       <div class="row_flex">
                           <div class="col-md-6 box_net_works">
                               <ul class="list__net_works">
                                   <li><a href="http://facebook.com"><i class="fab fa-facebook-f"></i></a></li>
                                   <li><a href="http://instagram.com"><i class="fab fa-instagram"></i></a></li>
                                   <li><a href="http://twitter.com"><i class="fab fa-twitter"></i></a></li>
                                   <li><a href="http://plus.google.com"><i class="fab fa-google-plus-g"></i></a></li>
                               </ul>
                           </div>
                           <div class="col-md-6 box_callBack">
                               <p class="phone"><i class="fas fa-phone"></i>+996 555 904 115</p>
                               <a href="#" class="btn_callBack" data-toggle="modal" data-target="#modalOrder">Обратный Звонок</a>
                           </div>
                       </div>
                   </div>
               </div>
