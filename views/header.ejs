                        </nav>
                        <div class="col-md-2 block__icon_user">
                            <div class="dropdown box__btn_search">
                                <a href="#" class="btn_user" data-toggle="dropdown">
                                    <i class="fas fa-search open"></i>
                                    <i class="closed">&times;</i>
                                </a>
                                <div class="dropdown-menu">
                                    <form action="">
                                        <div class="group">
                                            <input type="text" name="textfield" placeholder="Найти...">
                                            <button type="submit" class="btn_search"><i class="fas fa-search"></i></button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <div class="box__btn_basket">
                                <a href="/basket" class="btn_user"><i class="fas fa-shopping-basket"></i>
                                    <span class="number">3</span>
                                </a>
