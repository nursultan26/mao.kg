        <title>Главная</title>
        <link rel="stylesheet" type="text/css" href="<%= path %>stylesheets/css/libs.min.css">
        <link rel="stylesheet" type="text/css" href="<%= path %>stylesheets/css/main.css">
        <link rel="stylesheet" type="text/css" href="<%= path %>stylesheets/css/media.css">
        <!--Fonts-->
        <link href="https://use.fontawesome.com/releases/v5.0.1/css/all.css" rel="stylesheet">
